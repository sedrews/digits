def pre():
    modal = step([(0,1,1/3),(1,2,1/3),(2,3,1/3)])
    if modal < 1:
        lin = gaussian(30, 9)
    elif modal < 2:
        lin = gaussian(35, 9)
    else:
        lin = gaussian(50, 9)
    ltarget = gaussian(75, 1)
    return lin, ltarget

